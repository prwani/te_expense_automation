# Example environment configuration for Travel Expense Automation Agent
# Copy to .env and fill in actual values. Do NOT commit your real .env.

# SQLite database file path (optional; falls back to backend/app/db/expenses.db)
EXPENSE_DB_PATH=./backend/app/db/expenses.db

# Directory for uploaded receipt files
RECEIPT_UPLOAD_DIR=./backend/uploads

# Azure Document Intelligence (prebuilt-receipt model)
# Endpoint format: https://<resource-name>.cognitiveservices.azure.com/
AZURE_DOCUMENT_INTELLIGENCE_ENDPOINT=
# Leave key blank if using Managed Identity / DefaultAzureCredential
AZURE_DOCUMENT_INTELLIGENCE_KEY=
// Optional: choose a different model (prebuilt-receipt, prebuilt-invoice, custom model id)
AZURE_DOCUMENT_INTELLIGENCE_MODEL_ID=prebuilt-receipt

# Azure Content Understanding (preview) endpoint & key
# Endpoint example: https://<region>.api.cognitive.microsoft.com/ (adjust per service docs)
AZURE_CONTENT_UNDERSTANDING_ENDPOINT=
AZURE_CONTENT_UNDERSTANDING_KEY=

# Analyzer / Model ID for Content Understanding (e.g. prebuilt-invoice, custom analyzer id)
AZURE_CONTENT_UNDERSTANDING_ANALYZER_ID=prebuilt-invoice
AZURE_CONTENT_UNDERSTANDING_API_VERSION=2024-11-01-preview

# Logging level (optional)
LOG_LEVEL=INFO
